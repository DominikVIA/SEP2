<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="640.0" stylesheets="@../examMaster.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="en.via.sep2_exammaster.view.student.InfoExamViewController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <TextArea fx:id="contentArea" editable="false" layoutX="43.0" layoutY="93.0" prefHeight="60.0" prefWidth="289.0" text="Content">
               <font>
                  <Font name="Avenir Roman" size="13.0" />
               </font></TextArea>
            <TextField fx:id="titleField" editable="false" layoutX="43.0" layoutY="30.0" prefHeight="45.0" prefWidth="185.0" text="Title">
               <font>
                  <Font name="Avenir Heavy" size="21.0" />
               </font>
            </TextField>
            <Separator layoutX="28.0" layoutY="68.0" prefHeight="31.0" prefWidth="313.0" />
            <ListView fx:id="announcementsList" layoutX="376.0" layoutY="93.0" onMouseClicked="#onClick" prefHeight="170.0" prefWidth="225.0" styleClass="smallListView" />
            <Label layoutX="376.0" layoutY="61.0" prefHeight="35.0" prefWidth="162.0" styleClass="normalLabel" text="Announcements:">
               <font>
                  <Font name="Avenir Heavy" size="20.0" />
               </font>
            </Label>
            <TextField fx:id="timeField" editable="false" layoutX="43.0" layoutY="162.0" prefHeight="26.0" prefWidth="140.0" text="Time">
               <font>
                  <Font name="Avenir Roman" size="13.0" />
               </font>
            </TextField>
            <TextField fx:id="typeField" editable="false" layoutX="43.0" layoutY="199.0" prefHeight="26.0" prefWidth="140.0" text="Type">
               <font>
                  <Font name="Avenir Roman" size="13.0" />
               </font>
            </TextField>
            <TextField fx:id="examinerField" editable="false" layoutX="193.0" layoutY="199.0" prefHeight="26.0" prefWidth="140.0" text="Examiner">
               <font>
                  <Font name="Avenir Roman" size="13.0" />
               </font>
            </TextField>
            <Button fx:id="viewButton" layoutX="376.0" layoutY="268.0" mnemonicParsing="false" onAction="#onView" prefHeight="28.0" prefWidth="225.0" styleClass="normalButton" text="View">
               <font>
                  <Font name="Avenir Roman" size="13.0" />
               </font>
            </Button>
            <Label layoutX="43.0" layoutY="252.0" prefHeight="30.0" prefWidth="119.0" styleClass="normalLabel" text="Final grade: ">
               <font>
                  <Font name="Avenir Black" size="20.0" />
               </font>
            </Label>
            <TextArea fx:id="feedbackArea" editable="false" layoutX="43.0" layoutY="292.0" prefHeight="90.0" prefWidth="289.0" text="Feedback">
               <font>
                  <Font name="Avenir Roman" size="13.0" />
               </font>
            </TextArea>
            <Label fx:id="gradeLabel" layoutX="162.0" layoutY="252.0" prefHeight="30.0" prefWidth="155.0" styleClass="normalLabel" text=" ">
               <font>
                  <Font name="Avenir Black" size="20.0" />
               </font>
            </Label>
            <TextField fx:id="roomField" editable="false" layoutX="240.0" layoutY="47.0" prefHeight="28.0" prefWidth="90.0" text="Room">
               <font>
                  <Font name="Avenir Roman" size="13.0" />
               </font>
            </TextField>
            <Label layoutX="240.0" layoutY="30.0" styleClass="normalLabel" text="Room:" />
            <Button layoutX="376.0" layoutY="352.0" mnemonicParsing="false" onAction="#onBack" styleClass="normalButton" text="Back">
               <font>
                  <Font size="13.0" />
               </font>
            </Button>
            <TextField fx:id="dateField" editable="false" layoutX="193.0" layoutY="163.0" prefHeight="26.0" prefWidth="140.0" text="Date">
               <font>
                  <Font name="Avenir Roman" size="13.0" />
               </font>
            </TextField>
      </children>
    </AnchorPane>
  </children>
</VBox>
